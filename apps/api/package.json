{
  "name": "@gravyty/api",
  "version": "0.1.0",
  "description": "Gravyty Enablement Portal API",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "build:lambda": "npm run build && npm run bundle:lambda",
    "bundle:lambda": "rm -rf dist-lambda && mkdir -p dist-lambda && cp -r dist dist-lambda/ && cp package*.json dist-lambda/ && cd dist-lambda && npm ci --omit=dev",
    "start": "node dist/server.js",
    "typecheck": "tsc --noEmit",
    "test:ai": "tsx src/ai/test-ai-service.ts",
    "test:unsplash": "tsx src/handlers/test-unsplash.ts"
  },
  "keywords": [
    "api",
    "enablement"
  ],
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.958.0",
    "@aws-sdk/client-dynamodb": "^3.490.0",
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/client-sqs": "^3.490.0",
    "@aws-sdk/client-ssm": "^3.490.0",
    "@aws-sdk/client-transcribe": "^3.958.0",
    "@aws-sdk/lib-dynamodb": "^3.490.0",
    "@aws-sdk/s3-request-presigner": "^3.490.0",
    "@google-cloud/aiplatform": "^3.9.0",
    "google-auth-library": "^9.0.0",
    "googleapis": "^144.0.0",
    "@gravyty/domain": "*",
    "@opensearch-project/opensearch": "^2.5.0",
    "@vendia/serverless-express": "^4.12.6",
    "aws-jwt-verify": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/node": "^20.10.6",
    "@types/uuid": "^9.0.7",
    "tsx": "^4.21.0",
    "typescript": "^5.3.0"
  }
}
