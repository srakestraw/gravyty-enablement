# Storage Backend
# Options: 'stub' (in-memory) or 'aws' (DynamoDB + S3)
STORAGE_BACKEND=stub

# AWS Configuration
AWS_REGION=us-east-1

# DynamoDB Table Names
# These should match the table names created by CDK stack
DDB_TABLE_CONTENT=content_registry
DDB_TABLE_NOTIFICATIONS=notifications
DDB_TABLE_SUBSCRIPTIONS=subscriptions
DDB_TABLE_EVENTS=events

# S3 Bucket Name
# This should match the bucket name created by CDK stack
ENABLEMENT_CONTENT_BUCKET=

# Server Configuration
PORT=4000
